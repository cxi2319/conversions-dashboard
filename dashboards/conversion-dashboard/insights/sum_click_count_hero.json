{
    "$id": "sum_click_count_hero",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Sum of Click Count ",
    "description": "Sum of Click Count ",
    "query": {
      "metrics": [
        "yext/CLICK_COUNT"
      ],
      "dimensions": [
        "yext/DAYS"
      ],
      "filters": [
        {
          "filter": "yext/date",
          "in": "DAYS_7"
        }
      ]
    },
    "display": {
      "hero": {
        "aggregation": "SUM"
      }
    }
  }